{
	"version": 2,
	"buildCommand": "npm run build",
	"installCommand": "npm install",
	"builds": [
		{
			"src": "/src/*.ts",
			"use": "@vercel/node"
		},
        {
            "src": "/src/views/**",
            "use": "@vercel/static"
        }
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/dist/index.js"
		}
	]
}
